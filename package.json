{
  "name": "ipc-benchmark-testing",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "emit-types": "rm -rf ./types/raw && tsc --declaration --emitDeclarationOnly --declarationDir ./types/raw && rollup -c",
    "benchmark": "npm run tsc && node ./benchmark/main.js",
    "sync-data": "mkdir -p visualization-ui/src/data && rm -f visualization-ui/src/data/* && cp results/*.publish.json visualization-ui/src/data",
    "full-test-suite": "bash ./run-test-suite.sh",
    "test:benchmark": "npm run tsc && docker-compose -f ./benchmark/docker-compose.yml up",
    "test:http": "npm run tsc && docker-compose -f ./ipc-http/docker-compose.yml up",
    "test:http-express-axios": "npm run tsc && docker-compose -f ./ipc-http-express-axios/docker-compose.yml up"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anttispitkanen/ipc-benchmark-testing.git"
  },
